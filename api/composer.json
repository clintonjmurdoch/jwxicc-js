{
    "require": {
        "slim/slim": "^3.8",
		"oomphinc/composer-installers-extender": "^1.1",
        "bower-asset/bootstrap": "^3.1.1",
		"bower-asset/jquery": "^2.2",
		"bower-asset/angular": "^1.3.14",
		"bower-asset/angular-route": "^1.3.14"
    },
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	],
	"extra": {
	  "installer-types": [
		"bower-asset",
		"npm-asset"
	  ],
	  "installer-paths": {
		"../bower_components/{$name}": [
		  "type:bower-asset",
		  "type:npm-asset"
		]
	  }
	}
}